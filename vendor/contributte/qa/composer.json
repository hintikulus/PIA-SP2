{
  "name": "contributte/qa",
  "description": "Tuned & very strict coding standards for PHP projects. Trusted by Contributte, Apitte, Nettrine and many others.",
  "keywords": [
    "codesniffer",
    "codestyle",
    "contributte",
    "qa",
    "quality assurance"
  ],
  "type": "library",
  "license": [
    "MIT"
  ],
  "homepage": "https://github.com/contributte/qa",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.0",
    "slevomat/coding-standard": "^8.12.1",
    "squizlabs/php_codesniffer": "^3.7.2"
  },
  "require-dev": {
    "contributte/tester": "^0.2.0",
    "symfony/process": "^6.0.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "bin": [
    "bin/codesniffer",
    "bin/codefixer"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
